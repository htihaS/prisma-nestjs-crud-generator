{
	"name": "@arianaprojects/prisma-nestjs-crud-generator",
	"version": "0.4.12",
	"description": "code generator from Prisma schema",
	"main": "dist/index.js",
	"license": "Apache License 2.0",
	"homepage": "https://www.arianagermany.com",
	"repository": "https://github.com/ArianaProjects/prisma-nestjs-crud-generator",
	"keywords": [
		"prisma",
		"nestjs",
		"typescript",
		"generator",
		"crud"
	],
	"scripts": {
		"format": "prettier --write \"src/**/*.ts\"",
		"dev": "rm -rf ./gen && node -r ts-node/register src/index.ts",
		"start": "node dist/bin.js",
		"build": "rm -rf ./dist && rm -rf ./gen && npx tsc",
		"prepack": "yarn build",
		"release": "release-it",
		"test": "jest"
	},
	"bin": {
		"prisma-code-generator": "dist/bin.js"
	},
	"engines": {
		"node": ">=14"
	},
	"author": {
		"name": "shayan",
		"email": "shayandavarifard@yahoo.com"
	},
	"dependencies": {
		"@nestjs/common": "^10.3.3",
		"@nestjs/swagger": "^7.3.0",
		"@prisma/client": "^4.3.1",
		"@prisma/generator-helper": "^3.8.1",
		"@prisma/sdk": "^3.8.1",
		"change-case": "^4.1.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"json-schema": "^0.4.0",
		"nestjs-prisma": "^0.23.0",
		"prettier": "2.5.1"
	},
	"devDependencies": {
		"@ariana-germany/prisma-code-generator": "^0.2.13",
		"@types/jest": "^27.4.0",
		"@types/json-schema": "^7.0.9",
		"@types/node": "^16.6.2",
		"@types/prettier": "2.4.3",
		"jest": "^27.4.7",
		"prisma": "4.3.1",
		"release-it": "^14.12.4",
		"swagger-ui-express": "^4.1.6",
		"ts-node": "^10.2.1",
		"typescript": "^4.3.5"
	},
	"_moduleAliases": {
		"..": "dist"
	}
}
